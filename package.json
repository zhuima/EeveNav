    "better-sqlite3": "^12.2.0",
    "echarts": "^5.6.0",
    "highlight.js": "^11.11.1",
    "marked": "^16.0.0",
    "mermaid": "^11.8.1",
    "typescript": "^5.6.2"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^3.7.3",
